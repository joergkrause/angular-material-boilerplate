{
  "name": "angular-material-boilerplate",
  "version": "1.0.0",
  "description": "A demo project for Angular5, Material 5 and Flex Layout with Gulp/SystemJs bundling and treeshaking and optional AoT.",
  "main": "index.js",
  "scripts": {
    "build": "gulp clean && tsc && gulp",
    "aot": "gulp clean && ./node_modules/.bin/ngc -p ./tsconfig.aot.json && gulp aot",
    "start": "http-server ./dist -p 3001 -g -o index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
